        public static function isISBN13Valid(isbn:String):Boolean
        {
            var check:int = 0;
            for (var i:int = 0; i < 12; i += 2)
            {
                check += int(isbn.substring(i, i + 1));
            }
            for (i = 1; i < 12; i += 2)
            {
                check += int(isbn.substring(i, i + 1)) * 3;
            }
            check += int(isbn.substring(12));
            return check % 10 == 0;
        }
        
