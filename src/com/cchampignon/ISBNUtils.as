            var withoutHyphens:String = isbn.replace(RegExp(/-/g), "");
            var withHyphens:String = '';
            var index:int = 0;
            
            var GS1Prefix:String;
            if(withoutHyphens.length == 13)
            {
                GS1Prefix = withoutHyphens.substr(index,3);
                withHyphens = GS1Prefix + '-';
                index = 3;
            }
            else if(withoutHyphens.length == 10)
            {
                GS1Prefix = '978';
            }
            else
            {
                throw new Error("Length must be 10 or 13");
            }
            
            var first7:int = int(withoutHyphens.substr(index, 7));
            var groupPrefixLength:int = -1;
            if(!groupsLengthsTable.hasOwnProperty(GS1Prefix) )
                throw new Error("Unable to hyphenate");
            for(var i:int = 0; i < groupsLengthsTable[GS1Prefix].length && groupPrefixLength == -1; i++)
            {
                if(groupsLengthsTable[GS1Prefix][i]['min'] <= first7 &&
                    first7 <= groupsLengthsTable[GS1Prefix][i]['max'])
                    groupPrefixLength = groupsLengthsTable[GS1Prefix][i]['length']
            }
            if(groupPrefixLength == -1)
                throw new Error("Not in any recognized group range");
            else if(groupPrefixLength == 0)
                throw new Error("Group range is unused");
            var groupPrefix:String = withoutHyphens.substr(index, groupPrefixLength);
            index = index + groupPrefixLength;
            withHyphens += groupPrefix + '-';
            
            var next7:String = withoutHyphens.substr(index, 7);
            //pad out 7 places
            while (next7.length < 7)
            {
                next7 += "0";
            }
            var GS1andGroup:String = GS1Prefix + '-' + groupPrefix;
            var publisherLength:int = -1;
            if(!publisherLengthsTable.hasOwnProperty(GS1andGroup))
                throw new Error("Prefix " + GS1andGroup + "not recgonized");
            for(i = 0; i < publisherLengthsTable[GS1andGroup].length && publisherLength == -1; i++)
            {
                if(publisherLengthsTable[GS1andGroup][i]['min'] <= next7 &&
                    next7 <= publisherLengthsTable[GS1andGroup][i]['max'] )
                    publisherLength = publisherLengthsTable[GS1andGroup][i]['length']
            }
            if(publisherLength == -1)
                throw new Error("Not in any recognized publisher range");
            else if(publisherLength == 0)
                throw new Error("Publisher range is unused");
            
            var publisher:String = withoutHyphens.substr(index, publisherLength);
            index = index + publisherLength;
            withHyphens += publisher + "-";
            
            //book id is remaining numbers except last digit which is the check digit
            var end:int = withoutHyphens.length - 1;
            var bookId:String = withoutHyphens.substring(index, end);
            var checkDigit:String = withoutHyphens.substring(end);
            //All together now!
            withHyphens += bookId + "-" + checkDigit;
            
            
            return withHyphens;
        }
    }
}
